                if (uet && uex) {
                    uet('be', 'LoadBranchSDK', {wb: 1});
                    uex('ld', 'LoadBranchSDK', {wb: 1} )
                }
                if (err) {
                    console.log('Branch init error', err);
                    logBranchMetric('BranchSDK.Error', 1);
                } else {
                    logBranchMetric('BranchSDK.Error', 0);
                    logBranchMetric('BranchSDK.Initialized', 1);

