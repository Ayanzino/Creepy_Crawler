            function logBranchMetric(metricName, n) {
                if (window && window.ue && typeof window.ue.count === 'function') {
                    window.ue.count(metricName, n);
                }
            }

            logBranchMetric('BranchSDK.Requests', 1);
            if (uet) {
                uet('bb', 'LoadBranchSDK', {wb: 1});
            }
