<script src="https://google-code-prettify.googlecode.com/svn/loader/run_prettify.js?lang=css"></script>
<script>
$(function() {
    window.prettyPrint && prettyPrint()
    $(document).on('click', '.yamm .dropdown-menu', function(e) {
        e.stopPropagation()
    })
})

$(document).ready(function() {

    /**To be DESKTOP ONLY functionality**/

    $('.level-two').hover(function() {
        $('.level-three').removeClass('open');
        $('.menu-group').removeClass('open');
        $(this).parent().addClass('open');
        $(this).parent().find('.level-three').addClass('open');
    });


    $('a.dropdown-toggle').click(function(event) {
        if ($(window).width() > 990) {
            var link_location = $(this).attr('href');
            window.location.href = link_location;
        }
    });

    $('.dropdown').hover(function(event) {
            if ($(window).width() > 990) {
                $('.dropdown').each(function() {
                    $(this).removeClass('open');
                });
                $(this).addClass('open');
            }
        },
        function() {
            $('.dropdown').each(function() {
                $(this).removeClass('open');
            });
            $('.level-three').removeClass('open');
            $('.menu-group').removeClass('open');
        });

    /**End DESKTOP ONLY functionality**/

    $('.dropdown-toggle').click(function() {
        $('.menu-group').removeClass('open');

        $('.level-three').removeClass('open');

    });

});

</script>
<script>
$(document).ready(function() {
    $('#menu').slinky();
});

</script>
