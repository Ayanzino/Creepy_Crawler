<script>
(function ($) {
  // SCROLL TO TOP /////////////////////
  Drupal.behaviors.scrollTop = {
    attach: function (context, settings) {
         
			var offset = 700;
			var duration = 1000;
			$(window).scroll(function() {
				if ($(this).scrollTop() > offset) {
					$('.back-to-top').fadeIn(duration);
				} else {
					$('.back-to-top').fadeOut(duration);
				}
			});
			
			$('.back-to-top').click(function(event) {
				event.preventDefault();
				$('html, body').animate({scrollTop: 0}, duration);
				return false;
			});
				
    }
  };


// Controls the open and close top banner
  $("div.expandbutton").click(function(){
    $(".expandbutton-message").toggle();
    $(".expandmessage").toggle();
    $(".collpasebutton").toggle();
    $(".expandbutton").toggle();
    
  });

  $("div.expandbutton-message").click(function(){
    $(".expandbutton-message").toggle();
    $(".expandmessage").toggle();
    $(".collpasebutton").toggle();
    $(".expandbutton").toggle();
  });

  $("div.collpasebutton").click(function(){
    $(".expandbutton-message").toggle();
    $(".expandmessage").toggle();
    $(".collpasebutton").toggle();
    $(".expandbutton").toggle();
  });

})(jQuery);
</script>

