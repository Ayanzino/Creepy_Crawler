  <!-- include the jquery-accessibleMegaMenu plugin script -->
  <script src="/sites/all/themes/grants2019/templates/system/Accessible-Mega-Menu-master/js/jquery-accessibleMegaMenu.js"></script>



  <script>



$( document ).ready(function() {



if (window.location.href.indexOf("/grants/") > -1) {
var currentlocation = 'grants';
}

if (window.location.href.indexOf("/grants/oer.htm") > -1) {
var currentlocation = 'home';
}

if (window.location.href.indexOf("/funding/") > -1) {
var currentlocation = 'funding';
}

if (window.location.href.indexOf("/grants/funding/funding_program.htm") > -1) {
var currentlocation = 'grants';
}

if (window.location.href.indexOf("/policy/") > -1) {
var currentlocation = 'policy';
}

if (window.location.href.indexOf("/grants/natural_disasters/corona-virus.htm") > -1) {
var currentlocation = 'policy';
}

if (window.location.href.indexOf("/grants/natural_disasters.htm") > -1) {
var currentlocation = 'policy';
}

if (window.location.href.indexOf("/news/") > -1) {
var currentlocation = 'news';
}

if (window.location.href.indexOf("/aboutoer/") > -1) {
var currentlocation = 'aboutoer';
}

console.log('/' + currentlocation + '/');
  $('#' + currentlocation).toggleClass('main-nav-highlight');



// This sets a timer to stop the window resize from generating too many items which will repeat the open and close function if you go from full screen to mobile
var id;
$(window).resize(function() {
  clearTimeout(id);
  id = setTimeout(doneResizing, 500);

});

$( ".hamburger" ).hide();
$( ".searchbutton" ).hide();

$('.col-sm-12').removeClass('zindexminus1');
$('.col-sm-9').removeClass('zindexminus1');
$('.col-sm-6').removeClass('zindexminus1');

$( ".hamburger" ).click(function() {
            $( ".main-nav" ).slideToggle();
            $('.col-sm-12').toggleClass('zindexminus1');
            $('.col-sm-9').toggleClass('zindexminus1');
            $('.col-sm-6').toggleClass('zindexminus1');
});


if ($(window).width() <= 820) {
  $( ".main-nav" ).css({"display":"none"});
  $(".mobile-only").toggleClass("show");
  
$('.nav-item').addClass("alreadymobile");


  $('.sub-nav li').css({"max-width":"100%"});
  $( ".hamburger" ).show();
  $( ".searchbutton" ).show();
  $( ".openclose" ).show();

    $( "aside.sidebar-first" ).appendTo( "div.mobile_sidebar_first" );
    $( ".cross" ).hide();
    $( ".main-nav" ).hide();

    $( ".site-search" ).insertAfter( ".searchbutton" );
    $(".site-search").css('display', 'none');

    $(".searchsitetext").css('display', 'none');

    $(".searchbutton").click(function(){
      $( ".site-search" ).toggleClass( "show" );
      $(".site-search").toggleClass("site-search-mobile");
    });

   // Splits the blog and news area on the home page for mobile
   $(".two4-news").css('display', 'none');
    
    $( '<div class="two4-news2" style="display:inline; text-align:center"><a href="/news/news.htm"><img src="/sites/all/themes/grants2019/images/news.png" class="mikesblog" height="36px" alt="News article icon" data-alt-src="/themes/era2019/images/news.png">News </a></div>' ).insertAfter( ".two4" );



if (!$( ".nav-item" ).hasClass("resizedfrommoibletofull"))  {
 $( ".nav-item" ).click(function() {



        var link = $(this);
   $(this).find('.sub-nav').slideToggle('slow', function() {
      if ($(this).is(':visible')) {
           $(this).parent().find('span.openclose').html( '<i class="fas fa-angle-right openedmenu-fas-active"></i>' );
      } else {
           $(this).parent().find('span.openclose').html( '<i class="fas fa-angle-down"></i>' );
      }

      $(this).closest(".nav-item").toggleClass('openedmenu');
  $(this).closest(".nav-item").find('a:first').toggleClass('openedmenu-link-active');
  });


           });

             }

} else if ($(window).width() > 820) {



  $(".mobile-only").toggleClass("hide");

  $('.sub-nav li').css({"max-width":"400px"});

  $(".mobile-menu").toggleClass("hide");
  $( ".openclose" ).hide();

  // Splits the blog and news area on the home page for mobile
  

    $("nav:first").accessibleMegaMenu({
                      /* prefix for generated unique id attributes, which are required
                        to indicate aria-owns, aria-controls and aria-labelledby */
                      uuidPrefix: "accessible-megamenu",

                      /* css class used to define the megamenu styling */
                      menuClass: "nav-menu",

                      /* css class for a top-level navigation item in the megamenu */
                      topNavItemClass: "nav-item",

                      /* css class for a megamenu panel */
                      panelClass: "sub-nav",

                      /* css class for a group of items within a megamenu panel */
                      panelGroupClass: "sub-nav-group",

                      /* css class for the hover state */
                      hoverClass: "hover",

                      /* css class for the focus state */
                      focusClass: "focus",

                      /* css class for the open state */
                      openClass: "open"
                   });

}


// this code is OK to be executed several times when the user resizes the window
$(window).resize(function() {

//This was causing the mobile view menu to not work so leave commented out $( ".main-nav" ).hide();

$('.col-sm-12').toggleClass('zindexminus1');
$('.col-sm-9').toggleClass('zindexminus1');
$('.col-sm-6').toggleClass('zindexminus1');

var win = $(this); //this = window
if (win.width() <= 820) {


     $('.nav-item').addClass("resizedtomobile");

  $('.sub-nav li').css({"width":"100%"});

              $( "aside.sidebar-first" ).appendTo( "div.mobile_sidebar_first" );

                $( ".cross" ).hide();
              //  $( ".main-nav" ).hide();
                $( ".hamburger" ).show();
                $( ".searchbutton" ).show();
                $( ".openclose" ).show();

                $( ".site-search" ).insertAfter( ".searchbutton" );

 
    $(".site-search").css('display', 'none');
  
    $(".searchsitetext").css('display', 'none');

    $(".searchbutton").click(function(){
      $( ".site-search" ).toggleClass( "show" );
      $(".site-search").toggleClass("site-search-mobile");
    });

   

      //    $(".mobile-menu").toggleClass("hide");

         }
});


// This code gets executed after the time has stopped aka when the user has stopped resizing the window, if this is not present the open and close function repeats several times
function doneResizing(){
  
            var win = $(this); //this = window
            if (win.width() <= 820) {
              $('.grants-logo').css({"float":"none"});

              $('.col-sm-12').toggleClass('zindexminus1');
              $('.col-sm-9').toggleClass('zindexminus1');
              $('.col-sm-6').toggleClass('zindexminus1');


  $('.sub-nav li').css({"width":"100%"});
  $('.sub-nav li').css({"max-width":"100%"});

  //Destroy accessibility plugin on mobile
  $('.sub-nav').removeAttr('aria-expanded');

                     // Splits the blog and news area on the home page for mobile
                     $(".two4-news").css('display', 'none');
    
    $( '<div class="two4-news2" style="display:inline; text-align:center"><a href="/news/news.htm"><img src="/sites/all/themes/grants2019/images/news.png" class="mikesblog" height="36px" alt="News article icon" data-alt-src="/themes/era2019/images/news.png">News </a></div>' ).insertAfter( ".two4" );


// This code only gets executed if the user is on full screen then moves the window to mobile
        if($( ".nav-item" ).hasClass( "resizedtomobile" ) && (!$( ".nav-item" ).hasClass("alreadymobile")) && (!$( ".nav-item" ).hasClass("resizedfrommoibletofull")))  {
                          $( ".nav-item" ).click(function() {
                          var link = $(this);
                          $(this).find('.sub-nav').slideToggle('slow', function() {
                              if ($(this).is(':visible')) {
                                    $(this).parent().find('span.openclose').html( '<i class="fas fa-angle-right openedmenu-fas-active"></i>' );
                              } else {
                                  $(this).parent().find('span.openclose').html( '<i class="fas fa-angle-down"></i>' );
                              }
                          });
                              $(this).closest(".nav-item").toggleClass('openedmenu');
  $(this).closest(".nav-item").find('a:first').toggleClass('openedmenu-link-active');
                                  });
           }



         } else {


           // Splits the blog and news area on the home page for mobile    
           $( ".two4-news2" ).appendTo( ".two4" );
           $('.two4-news2').css({"box-shadow":"none"});
           

          $('.grants-logo').css({"float":"left"});
           $('.col-sm-12').removeClass('zindexminus1');
           $('.col-sm-9').removeClass('zindexminus1');
           $('.col-sm-6').removeClass('zindexminus1');


               


           $( ".site-search" ).appendTo( "#searcharea" );

          $('.nav-item').addClass("resizedfrommoibletofull");
          
          $('.sub-nav li').css({"max-width":"400px"});

                $( ".hamburger" ).hide();
                $( ".searchbutton" ).hide();
                
                $('.site-search').css("display", "block");
                $( ".main-nav" ).show();
                 $( ".openclose" ).hide();

                $( "aside.sidebar-first" ).appendTo( "div.revertback_sidebar_first" );
                $('.sub-nav').css("display", "");



              //  if( $('nav:first').data('plugin_accessibleMegaMenu') ){
                  //Restart the plugin

                 // $('nav:first').data('plugin_accessibleMegaMenu').init();


              //  } else {


                  $('.sub-nav').attr('aria-expanded', 'true');

                   $("nav:first").accessibleMegaMenu({
                      /* prefix for generated unique id attributes, which are required
                        to indicate aria-owns, aria-controls and aria-labelledby */
                      uuidPrefix: "accessible-megamenu",

                      /* css class used to define the megamenu styling */
                      menuClass: "nav-menu",

                      /* css class for a top-level navigation item in the megamenu */
                      topNavItemClass: "nav-item",

                      /* css class for a megamenu panel */
                      panelClass: "sub-nav",

                      /* css class for a group of items within a megamenu panel */
                      panelGroupClass: "sub-nav-group",

                      /* css class for the hover state */
                      hoverClass: "hover",

                      /* css class for the focus state */
                      focusClass: "focus",

                      /* css class for the open state */
                      openClass: "open"
                   });
            //     }





            }
}
















});


  </script>
