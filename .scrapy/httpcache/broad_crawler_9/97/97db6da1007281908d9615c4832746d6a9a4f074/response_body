var _dotDashPlayer = false;

scrollWatcher();
scrollForArrowWatch();

var height = 0;
setTimeout(function() {
    jQuery('#down_arrow').addClass('transition');
    height = jQuery(document).height()
}, 10);

if(jQuery('#home_circles').length > 0){
  if(!isMobile()){
    skrollr.init();
  }
}

if(jQuery('.tip-scroller').length > 0){
  // Should have one by default, on page load.
  setInterval(function(){
    goToNextTip();
  }, 2000);
}

if(jQuery('#things_we_know .blurb').length > 0){
  // Should have one by default, on page load.
  setInterval(function(){
    goToNextThingWeKnow();
  }, 4000);
}

function goToNextTip(){
  var active = jQuery('.tip-scroller-content.active');
  var next = active.next();
  if(next.length == 0){
    next = jQuery(jQuery('.tip-scroller-content')[0]);
  }
  active.fadeOut(function(){
    active.removeClass('active');
    next.fadeIn();
    next.addClass('active')
  });
}

function goToNextThingWeKnow(){
  var active = jQuery('#things_we_know .blurb span.active');
  var next = active.next();
  if(next.length == 0){
    next = jQuery(jQuery('#things_we_know .blurb span')[0]);
  }
  active.removeClass('active');
  setTimeout(function(){
    next.addClass('active')
  }, 200);
}


jQuery('#magellan_inner').bind('inview', function (event, visible, topOrBottomOrBoth) {

  if (visible == true) {
    // element is now visible in the viewport
    if (topOrBottomOrBoth == 'top') {
      // top part of element is visible
    } else if (topOrBottomOrBoth == 'bottom') {
      // bottom part of element is visible
    } else {
      // whole part of element is visible
      jQuery('#magellan_inner').attr('style', 'background-image:url("/assets/img/dash/magellan/Magellan_Anim_20171221.gif")');
    }
  } else {
    // element has gone out of viewport
  }

});

jQuery('.toggle-dash-nav').click(function(e) {

  // Only do this on mobile.
  // Our nav is always visible on desktop.
  if(!isMobile()){
    return;
  }

  // If you click the logo that should take you home.
  if(jQuery(this).hasClass('dash-menu-label')){
    return;
  };

    if (jQuery('body').hasClass('dash-nav-open')) {
        hideDashModal();
        e.preventDefault();
        return;
    }
    showDashToggle(jQuery('.dash-nav'));
    jQuery('body').addClass('dash-nav-open').addClass('ot');
    setTimeout(function() {
        jQuery('body').addClass('transition-left');
        jQuery('.wrapper').bind('click.nav', function(e) {
            var target = jQuery(e.target);
            if (target.hasClass('dash-nav-links')) {
                return;
            }
            if (target.hasClass('dash-nav')) {
                return;
            }
            if (target.hasClass('nav-burger')) {
                return;
            }
            if (target.hasClass('primary-dash-nav-link')) {
                return;
            }
            if (target.hasClass('nav-li')) {
                return;
            }
            hideDashModal();
        });
    }, 10);
    e.preventDefault();
});

jQuery('#view_all_dash_press').click(function(e) {
    showDashToggle(jQuery('.in-the-news-lightbox'));
    e.preventDefault();
});

jQuery('.member .full-cover').click(function(e) {
    var target = jQuery(jQuery(this).attr('href'));
    showDashToggle(target);
    e.preventDefault();
});

jQuery('.dash-close-modal-link').click(function(e) {
    hideDashModal();
    e.preventDefault();
});


jQuery('#close_video').click(function(e){
  jQuery('.embed-container').remove();
  jQuery('.big-video-section').removeClass('showing-video');
});

jQuery(window).scroll(jQuery.throttle(20, scrollWatcher));
jQuery(window).scroll(jQuery.throttle(20, scrollForArrowWatch));

jQuery('#play-dotdash-video').click(function(e) {
  jQuery('.big-video-section').append('<div class="embed-container"><iframe id="dotdash_video" src="https://player.vimeo.com/video/215571869" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe></div>');
    _dotDashPlayer = new Vimeo.Player(document.getElementById('dotdash_video'));
    _dotDashPlayer.play();
    jQuery('.big-video-section').addClass('showing-video');
});

function scrollWatcher() {
    var top = jQuery(window).scrollTop();
    if (top > 50) {
        jQuery('body').addClass('scrolled')
    } else {
        jQuery('body').removeClass('scrolled')
    }
}

function scrollForArrowWatch(){
  var top = jQuery(window).scrollTop();
    if (top > 100) {
        jQuery('#down_arrow').addClass('hide')
    } else {
        jQuery('#down_arrow').removeClass('hide')
    }
    if (top > height / 2) {
        jQuery('#up_arrow').removeClass('hide')
    } else {
        jQuery('#up_arrow').addClass('hide')
    }
}

function isMobile(){
 return window.getComputedStyle(document.querySelector('body'), ':before').getPropertyValue('content').replace(/\"/g, '') === "mobile";
}

function showDashToggle(el) {
    jQuery('body').addClass('dash-modal-show');
    el.addClass('show');
}

function hideDashModal() {
    jQuery('body').removeClass('dash-modal-show').removeClass('dash-nav-open');
    setTimeout(function() {
        jQuery('body').removeClass('ot');
    }, 210); // Need to remove it after we've animated the nav back.
    jQuery('.dash-modal.show').removeClass('show');
    jQuery('.wrapper').unbind('click.nav');
}

</script>


