    };

    gdprDeny = function (e) {

        window['ga-disable-UA-27582338-28'] = true;

        if (typeof fbq !== 'undefined')
            fbq('consent', 'revoke');
    };

    document.addEventListener ? document.addEventListener('gdprAllow', gdprAllow, false) : document.attachEvent('gdprAllow', gdprAllow);
    document.addEventListener ? document.addEventListener('gdprDeny', gdprDeny, false) : document.attachEvent('gdprDeny', gdprDeny);
</script>

</body>
</html>
