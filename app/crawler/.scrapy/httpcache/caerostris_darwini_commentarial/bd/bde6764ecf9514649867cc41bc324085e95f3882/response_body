<script type="text/javascript">
var $jq = jQuery.noConflict();



/*
* Match the heights of selected elements on window resize and page load
*/
matchHeights();
$jq( window ).resize(function() {
    matchHeights();
});
function matchHeights(){
var maxHeight = 0;

    if($jq( window ).width() > 400){
        $jq('.gel-layout__item .clickable-tile .tile-text').each(function(){
            maxHeight = $jq(this).height() > maxHeight ? $jq(this).height() : maxHeight;
        });
        $jq('.gel-layout__item .clickable-tile .tile-text').each(function(){
            $jq(this).css('height', maxHeight);
        });
    }else{
        $jq(this).css('height', "auto");
    }
}
</script>
