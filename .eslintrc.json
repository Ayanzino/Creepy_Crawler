{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "settings": {
        "import/resolver": {
            "node": {},
            "babel-module": {
                "root": ["./react-app"]
            }
        }
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    }
}
